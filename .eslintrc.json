{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": [
  "next/core-web-vitals",
  "plugin:@typescript-eslint/recommended",
  "@lnu"
  ],
  "rules": {
    "semi": ["error", "always"],
    "indent": ["error", 2]
  },
  "plugins": [
    "testing-library",
    "@typescript-eslint"
  ],
  "ignorePatterns": ["**/__tests__/**"]
}
